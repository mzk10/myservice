<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mh.myservice.entity.VolEntity" table="vol" schema="xiezuo">
        <id name="id" column="id">
            <generator class="uuid"/>
        </id>
        <property name="bookid" column="bookid"/>
        <property name="content" column="content"/>
        <property name="volName" column="volName"/>
        <property name="volcount" column="volcount"/>
        <property name="createtime" column="createtime"/>
        <property name="edittime" column="edittime"/>
        <property name="line" column="line"/>
    </class>
</hibernate-mapping>